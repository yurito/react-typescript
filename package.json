{
  "name": "site",
  "version": "0.1.0",
  "homepage": "https://yurito.github.com/site/",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.17",
    "@types/node": "^12.7.2",
    "@types/react-dom": "16.8.5",
    "gh-pages": "^2.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router": "^5.1.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.1.2",
    "react-vanilla-tilt": "1.0.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build:css": "node-sass src/styles/scss -o src/styles/css ",
    "watch:css": "npm run build:css && node-sass src/styles/scss -o src/styles/css -w -r",
    "start:js": "react-scripts start",
    "start": "npm-run-all -p watch:css start:js",
    "build:js": "react-scripts build",
    "build": "npm-run-all build:*",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@react-vertex/color-hooks": "^1.8.0",
    "@react-vertex/core": "^2.9.0",
    "@react-vertex/dom-hooks": "^1.11.0",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.43",
    "d3-timer": "^1.0.9",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "react-reveal": "^1.2.2",
    "react-vertex": "^2.0.0"
  }
}
